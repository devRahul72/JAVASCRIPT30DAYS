//Using switch statement to print the day of the week based on the day number;
//test 1;
let dayNumber = 2;
switch (dayNumber) {
  default:
    dayNumber === 1 ?console.log("Monday"):null;
    dayNumber === 2 ?console.log("Tuesday"):null;
    dayNumber === 3 && console.log("Wednesday");
    dayNumber === 4 && console.log("Thursday");
    dayNumber === 5 && console.log("Friday");
    dayNumber === 6 && console.log("Saturday");
    dayNumber === 7 && console.log("Sunday");
}

